<UserControl x:Class="Southwind.Windows.Controls.Shipper"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Southwind.Entities;assembly=Southwind.Entities"
    m:Common.TypeContext="{x:Type d:ShipperEntity}"
    MinWidth="300">
    <StackPanel>
        <m:ValueLine m:Common.Route="CompanyName" />
        <m:ValueLine m:Common.Route="Phone" />
    </StackPanel>
</UserControl>
